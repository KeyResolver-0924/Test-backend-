{
    "builds": [
        {
            "src": "src/api/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "installCommand": "pip install -r requirements.txt",
                "maxDuration": 20
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api/main.py"
        }
    ],
    "env": {
        "PORT": "8000",
        "PYTHONPATH": "src"
    },
    "regions": ["arn1"]
}